
<link href="@Url.Content("~")Content/SiteMobile.css" rel="stylesheet" type="text/css">
@{
    ViewBag.Title = "Content Manager Mobile";
    Layout = "~/Views/Shared/_Layout.Mobile.cshtml";
}

<script>
    var HPRMWebConfig = {        
        webClientClassicUrl: "@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.WebClientClassicUrl",
        searchAhead: "@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.SearchAhead",
        useADFS: ("@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.UseADFS".toLowerCase() == "true"),
        virtualDirectory: '@Url.Content("~")'
    }
</script>
@{
    var objectsToLoad = new List<HP.HPTRIM.ServiceModel.BaseObjectTypes>() {
                HP.HPTRIM.ServiceModel.BaseObjectTypes.Record,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.Location,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.Workflow,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.SavedSearch,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.Activity,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.Request,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.EmailLink,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.TodoItem,
                HP.HPTRIM.ServiceModel.BaseObjectTypes.TodoItemItemReference
           
		};

    Html.RenderPartial("~/Views/Shared/_Initialisation.cshtml", objectsToLoad);
}

@Scripts.Render("~/bundles/HPRM")

    </head>

    <body class="as-spa">
        <div id="HPRecordsManagerError" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="false">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header alignItemsMiddle" style="margin-top:20px;">
						<img src="data:image/png;base64," />
                    </div>

                    <div class="modal-body alignItemsMiddle" style="margin-top:20px;">
                        <span>
                            <div id='errorBox' style="font-size:large; display:inline-block"></div>
                            @{
                            if (Request.Browser.Browser == "IE")
                            {
                                if (Request.Browser.MajorVersion < 9)
                                {
                                    Write("The minimum supported version for Content Manager is Internet Explorer 9, please upgrade to the latest version.");
                                }
                            }
                        }
                        </span>
                    </div>
                </div>
            </div>
        </div>
        <div id="bodyContent" style="height:100%;">
            <!-- ko template: {name: templateName(), data: $data, afterrender: afterLoad()} --><!-- /ko  -->
            <!-- ko template: {name: $data.notificationWindow.templateName(), data: $data.notificationWindow} --><!-- /ko  -->
        </div>
        <script>

            try {

                HP.HPTRIM.TrimClient.handleAjaxError = function (err) {
                    $("#errorBox").html(err.Message);
                    $('#HPRecordsManagerError').modal('show');
                }
    } catch (e) {
        //HP.HPTRIM.TrimClient.handleAjaxError probably hasn't been loaded by now.
        $('#HPRecordsManagerError').modal('show')
    }


    $(document).bind("mobileinit", function () {
        $.mobile.touchOverflowEnabled = true;
    });

    document.addEventListener('focusout', function (e) { window.scrollTo(0, 0) });

    $(document).ready(function () {
        infuser.defaults.templateSuffix = ".tmpl.html";
        infuser.defaults.templateUrl = HPRMWebConfig.virtualDirectory + "Content";

        var root = {};
        var trimClient = new HP.HPTRIM.TrimClient();

        // at this point check if we are on some kind of phone or not...
        // TODO - add query to force us not to
        var root = {};

        var trimClient = new HP.HPTRIM.TrimClient();
        trimClient.setDatabase(HPRM_currentDatabase);
        trimClient.setCurrentUser(HPRM_currentUser);
        trimClient.setCurrentUserMemberships(HPRM_currentUserMemberships);
        trimClient.setStartupOptions(HPRM_startupOptions);
        trimClient.setLocaleData(HPRM_localeOptions);
        trimClient.setPropertyDefCache(HPRM_propertyDefs);
        trimClient.setEnumCache(HPRM_enumDefs);
        trimClient.setCheckoutCaptionData(HPRM_checkoutCaption);
        trimClient.setObjectDefs(HPRM_objectDefs);
        trimClient.setFormDefintions(HPRM_objectFormDefinitionDefs);
        trimClient.addSearchClauseHelpToCache("Record", HPRM_recordSearchClauseDefs)

        trimClient.setWebClientOptions(HPRM_webClientOptions);

        root = new HP.HPTRIM.App.TRIMMobileApp(trimClient);
        window.root = root;
        ko.applyBindings(root);
        HP.HPTRIM.Utils.UrlHelper.processUrlQuery(trimClient);
        //Do Menu binding
        setTimeout(function () {
            $('#side-menu').metisMenu();
        }, 1500);

    });

    // Android 4.1 browser fix for Bootstrap 3.0
    //http://getbootstrap.com/getting-started/#support-android-stock-browser
    var nua = navigator.userAgent
    var isAndroid = (nua.indexOf('Mozilla/5.0') > -1 && nua.indexOf('Android ') > -1 && nua.indexOf('AppleWebKit') > -1 && nua.indexOf('Chrome') === -1)
    if (isAndroid) {
        $('select.form-control').removeClass('form-control').css('width', '100%')
    }

    </script>
</body>