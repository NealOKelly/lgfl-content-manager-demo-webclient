@{
	ViewBag.Title = "Setup Information";
}
@Styles.Render("~/Content/styles")

<script>
    var HPRMWebConfig = {        
    	
        softwareVersion: "@HP.HPTRIM.Web.SoftwareVersion.AssemblyVersion",
	    virtualDirectory: '@Url.Content("~")'
	}

</script>

<body class="as-spa" style="background-color:#FFFFFF">
	<div class="branding-border-top"></div>
	<div style="background-color:#0073E7">
		<img data-bind="attr:{alt: HP.HPTRIM.Messages.web_HPRM}" src="data:image/png;base64," />
        <h2 style="color:#fff;display:inline;vertical-align: middle;margin-left: 13px;">Content Manager Setup Information</h2>
	</div>
	<table class="table" style="margin:25px;">
		<thead>
			<tr>
				<th>Property</th>
				<th>Value</th>
			</tr>
		</thead>

		<tbody>	
		<!-- ko foreach:properties() -->
			<tr>
				<td data-bind="text: caption">Caption</td>
				<td data-bind="text: value">Value</td>
			</tr>
		<!-- /ko -->
		</tbody>
	</table>
	<div class="branding-border-bottom"></div>
</body>

@Scripts.Render("~/bundles/HPRM")

<script>
	var setupInfo = new HP.HPTRIM.Setup.SetupInfo();
	ko.applyBindings(setupInfo);
</script>