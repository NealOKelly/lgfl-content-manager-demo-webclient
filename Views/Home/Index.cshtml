    <div id="HPRecordsManagerError" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header alignItemsMiddle" style="margin-top:20px;">
                    <img data-bind="attr:{alt: HP.HPTRIM.Messages.web_HPRM}" src="data:image/png;base64," />
                </div>
                <div class="modal-body alignItemsMiddle" style="margin-top:20px;">

                    <div id='errorBox' style="font-size:large; display:inline-block"></div>
                    <br />

                </div>
            </div>
        </div>
    </div>


    <div id="bodyContent" style="height:100%;" role="main">
        <!-- ko template: {name: templateName(), data: $data, afterrender: $data.afterLoad()} --><!-- /ko  -->
        <!-- ko template: {name: $data.notificationWindow.templateName(), data: $data.notificationWindow} --><!-- /ko  -->
        <!-- ko template: {name: $data.progressWindow.templateName(), data: $data.progressWindow} --><!-- /ko  -->
    </div>

    <div id="hprm-dynamic-modal-progress" class="modal fade hprm-modal " tabindex="-1" role="dialog" aria-labelledby="HPRMModalOverlayLabel" aria-hidden="true" data-backdrop="static" data-keyboard="true">
    </div>

	

    <script>

		isBrowserSupported = function () {
			if ($.browser) {
				if ($.browser.msie) {
					var version = parseInt($.browser.version);
					if (version < 9) {
						return false;
					}
				}
			}
			return true;
		}

		try {

			HP.HPTRIM.TrimClient.handleAjaxError = function (err) {
				$('#HPRecordsManagerLoadScreen').modal('hide');
				$("#errorBox").html(err.Message);
				$('#HPRecordsManagerError').modal('show');
			}
		} catch (e) {
			//HP.HPTRIM.TrimClient.handleAjaxError probably hasn't been loaded by now.
			$('#HPRecordsManagerError').modal('show')
		}



		var initialiseApplication = function () {

		    HPRMWebConfig.antiForgeryToken = HPRM_antiForgeryToken;

		    if (HPRM_errorInitialising) {
                var errorMsg = HPRM_unableToConnectMessage;
		        HP.HPTRIM.TrimClient.handleAjaxError({ Message: errorMsg });
		        return;
		    }

		    if (!isBrowserSupported()) {
		        var errorMsg = HP.HPTRIM.StringUtils.format(HPRM.Messages.web_minimumBrowserVersion, '9');
		        HP.HPTRIM.TrimClient.handleAjaxError({ Message: errorMsg });
		        return;
		    }

		    infuser.defaults.templateSuffix = ".tmpl.html";
		    infuser.defaults.templateUrl = HPRMWebConfig.virtualDirectory + "Content";

		    var root = {};

		    var trimClient = new HP.HPTRIM.TrimClient();

		    trimClient.setDatabase(HPRM_currentDatabase);
		    trimClient.setCurrentUser(HPRM_currentUser);
		    trimClient.setCurrentUserMemberships(HPRM_currentUserMemberships);
		    trimClient.setStartupOptions(HPRM_startupOptions);
		    trimClient.setStartupOptionsDef(HPRM_userOptionDefs.Startup)
		    trimClient.setLocaleData(HPRM_localeOptions);
		    trimClient.setFileDropOptions(HPRM_fileDropOption);
		    trimClient.setSearchOptionDefinitions(HPRM_searchOptionDefs);
		    trimClient.setWebClientOptions(HPRM_webClientOptions);
		    trimClient.getAdditionaFields();

		    trimClient.setCheckoutCaptionData(HPRM_checkoutCaption);

		    trimClient.setObjectDefs(HPRM_objectDefs);
		    trimClient.initialiseMainObjectDefinitions(HPRM_objectDefs, HPRM_RequiredMainObjectPropertyDefs);
		    trimClient.setDefaultClientTimezoneIfEmpty(HPRM_webClientOptions);

		    $('#HPRecordsManagerLoadScreen').modal('hide');

		    root = new HP.HPTRIM.App.TRIMApp(trimClient);
		    window.root = root;
		    ko.applyBindings(root);
		    HP.HPTRIM.Utils.UrlHelper.processUrlQuery(trimClient);

		}


		$(document).ready(function () {

		    $('#HPRecordsManagerLoadScreen').modal('show');
		    var path = window.location.pathname;
		    // Remove trailing 'Home' if required.
            var desktopRoute = "Desktop"

		    if (path.length > desktopRoute.length) {
		        var index = path.indexOf(desktopRoute, path.length - desktopRoute.length);
		        if (index != -1)
		            path = path.substr(0, path.length - desktopRoute.length)
		    }

			try{
				var messageService = new HP.HPTRIM.MessageService.WebHub();
				messageService.start();
			}
			catch(e)
			{
				console.log("Error starting the WebHub service: " + e);
			}


		    initialiseApplication();



		});

		// Android 4.1 browser fix for Bootstrap 3.0
		//http://getbootstrap.com/getting-started/#support-android-stock-browser
		var nua = navigator.userAgent
		var isAndroid = (nua.indexOf('Mozilla/5.0') > -1 && nua.indexOf('Android ') > -1 && nua.indexOf('AppleWebKit') > -1 && nua.indexOf('Chrome') === -1)
		if (isAndroid) {
			$('select.form-control').removeClass('form-control').css('width', '100%')
		}


		


            
          
    </script>

</body>





