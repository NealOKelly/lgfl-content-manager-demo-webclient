@Styles.Render("~/Content/styles")
@Styles.Render("~/Content/images-style")
@{
	ViewBag.Title = "Content Manager";
}
<scripts src="~/scripts/jquery.signalR-2.4.0.js"></scripts>




<script>


	var HPRMWebConfig = {
		searchAhead: ("@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.SearchAhead".toLowerCase() == "true"),
		virtualDirectory: '@Url.Content("~")',
		useADFS: ("@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.UseADFS".toLowerCase() == "true"),
		requireAntiForgeryToken: ("@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.RequireAntiForgeryToken".toLowerCase() == "true"),
		isDebugEnabled: ('@( ((System.Web.Configuration.CompilationSection)System.Configuration.ConfigurationManager.GetSection(@"system.web/compilation")).Debug)' === 'True'),
		recursiveSymbol: ("@HP.HPTRIM.Web.Configuration.ServiceDefaults.Instance.Searching.SearchRecursiveOption"),
		disableDownloadDocument: ("@HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.DisableDownloadDocument".toLowerCase() == "true"),
		delayUploadedFileTransferTime: @HP.HPTRIM.Services.Common.ServiceConfiguration.Instance.Setup.DelayUploadedFileTransferTime.Value
    }

    var WebDAVConfig = {
        webdavEnabled: browserHasSPActiveX() && @HP.HPTRIM.WebClient.WebDAV.WebDAVEditService.isEnabled().ToString().ToLower(),
        webdavCheckOutPath: "@HP.HPTRIM.WebClient.WebDAV.WebDAVEditService.WEBDAV_CHECKOUT_PATH",
		webdavSPObj: "@HP.HPTRIM.WebClient.WebDAV.WebDAVEditService.WEBDAV_SP_EDIT_OBJ"
    }

	var WOPIConfig = {
		enabled: @HP.HPTRIM.WebClient.WOPI.WOPIService.isEnabled().ToString().ToLower(),
		checkoutType: "@HP.HPTRIM.WebClient.WOPI.Constants.CheckoutType",
		servicePrefix: "@HP.HPTRIM.WebClient.WOPI.Constants.PluginPrefix"
	}

    // Return true if the browser supports the SharePoint.OpenDocuments ActiveX object
	function browserHasSPActiveX()
	{
		try
		{
			var spObj = "@HP.HPTRIM.WebClient.WebDAV.WebDAVEditService.WEBDAV_SP_EDIT_OBJ"
			return new ActiveXObject(spObj) != null;
		}
		catch (ex)
		{
			return false;
		}
	}
	@{
        var objectsToLoad = new List<HP.HPTRIM.ServiceModel.BaseObjectTypes>() {
        HP.HPTRIM.ServiceModel.BaseObjectTypes.Record,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.Location,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.Workflow,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.SavedSearch,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.Activity,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.Request,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.CheckinStyle,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.TodoItem,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.TodoItemItemReference,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.ActionDef,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.ActionDefStep,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.RecordAction,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.UserLabel,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.History,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.ConsignmentApprover,
        HP.HPTRIM.ServiceModel.BaseObjectTypes.ConsignmentRejection,
        };

        Html.RenderPartial("~/Views/Shared/_Initialisation.cshtml", objectsToLoad);

	}


</script>

@Scripts.Render("~/bundles/HPRM")



</head>


<body class="as-spa" role="application">


    <div class="text-center" style="padding-top:30px" id="loading-status">
        
